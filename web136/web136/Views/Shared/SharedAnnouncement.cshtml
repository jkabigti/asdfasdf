@{
    ViewBag.Title = "SharedAnnouncement";
}

<h2>Announcement:</h2>
<div id="divSharedAnnouncementContent">
    <table>
        <thead>
            <tr>
                <th>Announcement</th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: viewModel">
            <tr>
                <td data-bind="text: text"></td>
                <td data-bind="text: date"></td>
            </tr>
        </tbody>
    </table>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        require(['ViewModels/AdminViewModel'], function (adminViewModel) {
            var adminViewModelObj = new adminViewModel();
            adminViewModelObj.LoadSharedAnnouncements();
        });
    });
</script>
