@{
    var id = ViewBag.id;
}
<h3>Edit Student Information</h3>

<div id="divEditStudentRecord">
    Id : <input type="text" data-bind="value: $data.id" />
    Ex: A123456
    <br /><br />
    SSN : <input type="text" data-bind="value: $data.ssn" />
    Ex: 123-12-1234
    <br /><br />
    First : <input type="text" data-bind="value: $data.first" />
    Ex: John
    <br /><br />
    Last : <input type="text" data-bind="value: $data.last" />
    Ex: Doe
    <br /><br />
    Email : <input type="text" data-bind="value: $data.email" />
    Ex: abc@ucsd.edu
    <br /><br />
    Password : <input type="text" data-bind="value: $data.password" /><br />
    <br />
    Shoe Size : <input type="text" data-bind="value: $data.shoesize" />
    <br /><br />
    Weight : <input type="text" data-bind="value: $data.weight" /><br />
    <br />
    <input type="button" data-bind="value: 'Update', click: $data.update" style="margin-left: 80px" />
    <br /><br />
    <div id="divEditMessage"></div>
</div>
